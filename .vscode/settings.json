{
  "editor.fontSize": 14,
  "window.zoomLevel": 0,
  "editor.tabSize": 2,
  "editor.smoothScrolling": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "never"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
  },
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore"
}