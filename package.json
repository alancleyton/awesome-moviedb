{
  "name": "awesome-moviedb",
  "description": "Movie database web application",
  "author": "Alan Cleyton <alancleyton67@gmail.com>",
  "repository": "git@github.com:alancleyton/awesome-moviedb.git",
  "license": "MIT",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "CI=false && tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .js,.jsx,ts,.tsx --fix --quiet --ignore-path .gitignore",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx}\"",
    "lint": "yarn lint:format && yarn lint:fix",
    "prepare": "husky install",
    "test": "jest",
    "test:changed": "jest -o",
    "test:coverage:report": "jest --ci --maxWorkers=2 --collectCoverage",
    "test:coverage": "jest --coverage --colors",
    "test:staged": "jest --ci",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "postcss-nesting": "^12.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.15.0",
    "sass": "^1.66.1",
    "styled-components": "^5.3.5",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^17.0.38",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "c8": "^7.11.3",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.3",
    "postcss": "^8.4.24",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vitest": "^0.13.1"
  }
}
