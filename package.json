{
  "name": "awesome-moviedb",
  "description": "Movie database web application",
  "author": "Alan Cleyton <alancleyton67@gmail.com>",
  "repository": "git@github.com:alancleyton/awesome-moviedb.git",
  "license": "MIT",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .js,.jsx,ts,.tsx --fix --quiet --ignore-path .gitignore",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx}\"",
    "lint": "yarn lint:format && yarn lint:fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/node": "^17.0.38",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.0",
    "c8": "^7.11.3",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.3",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vitest": "^0.13.1"
  }
}
